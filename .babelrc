{
  "env": {
    "development": {
      "presets": [
        [
          "env",
          {
            "loose": true,
            "targets": [
              "last 2 Chrome versions",
              "last 2 Edge versions",
              "Explorer >= 11",
              "last 2 Firefox versions",
              "last 2 Safari versions",
              "last 2 iOS versions",
              "Android >= 4.4",
              "last 2 ChromeAndroid versions"
            ]
          }
        ]
      ],
      "plugins": [
        "lodash"
      ]
    },
    "production": {
      "presets": [
        [
          "env",
          {
            "loose": true,
            "targets": [
              "last 2 Chrome versions",
              "last 2 Edge versions",
              "Explorer >= 11",
              "last 2 Firefox versions",
              "last 2 Safari versions",
              "last 2 iOS versions",
              "Android >= 4.4",
              "last 2 ChromeAndroid versions"
            ]
          }
        ]
      ],
      "plugins": [
        "unassert",
        "lodash"
      ]
    }
  }
}
