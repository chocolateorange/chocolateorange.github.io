{
  "dependencies": {
    "domready": "^1.0.8",
    "fallback-preload": "github:sasaplus1-prototype/fallback-preload.js",
    "lodash": "^4.17.4",
    "parse-query": "github:sasaplus1-prototype/parse-query.js"
  },
  "devDependencies": {
    "amp": "file:local_modules/amp",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-unassert": "^2.1.2",
    "babel-preset-env": "^1.4.0",
    "eslint": "^3.15.0",
    "fixpack": "^2.3.1",
    "html-loader": "^0.4.4",
    "husky": "^0.13.4",
    "json-loader": "^0.5.4",
    "new": "file:local_modules/new",
    "npm-run-all": "^4.1.1",
    "publish": "file:local_modules/publish",
    "stylelint": "^7.8.0",
    "stylelint-config-standard": "^16.0.0",
    "stylelint-processor-ignore-front-matter": "^0.1.0",
    "uglify-save-license": "^0.4.1",
    "webpack": "^2.2.1"
  },
  "engines": {
    "node": ">=8.4.0",
    "npm": ">=5.4.1"
  },
  "private": true,
  "scripts": {
    "amp": "amp \"$(npm prefix)\" && bundle exec sass --load-path ./_sass --style compressed --scss _includes/amp/amp.scss | sed $'1s/^\\xEF\\xBB\\xBF//' > _includes/amp/amp.css",
    "build": "bundle exec jekyll build",
    "compile": "npm run webpack:compile",
    "develop": "npm run webpack:develop",
    "eslint": "eslint ./javascript",
    "fixpack": "fixpack",
    "install:gem": "bundle install --path vendor/bundle",
    "minify": "npm run webpack:minify",
    "new": "new \"$(npm prefix)\"",
    "preview": "bundle exec jekyll serve --drafts --watch",
    "publish": "publish \"$(npm prefix)\"",
    "stylelint": "stylelint ./_sass/*.scss ./css/*.scss",
    "test": "run-p --aggregate-output eslint stylelint",
    "webpack": "webpack --colors --display-error-details --progress",
    "webpack:compile": "npm run webpack -- --devtool inline-source-map",
    "webpack:develop": "npm run webpack:compile -- --watch",
    "webpack:minify": "NODE_ENV=production npm run webpack -- --optimize-minimize"
  }
}
