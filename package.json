{
  "babel": {
    "env": {
      "development": {
        "presets": [
          [
            "env",
            {
              "targets": [
                "last 2 Chrome versions",
                "last 2 Edge versions",
                "last 2 Firefox versions",
                "last 2 Safari versions",
                "last 2 iOS versions",
                "last 2 ChromeAndroid versions"
              ]
            }
          ]
        ],
        "plugins": [
          "lodash"
        ]
      },
      "production": {
        "presets": [
          [
            "env",
            {
              "targets": [
                "last 2 Chrome versions",
                "last 2 Edge versions",
                "last 2 Firefox versions",
                "last 2 Safari versions",
                "last 2 iOS versions",
                "last 2 ChromeAndroid versions"
              ]
            }
          ]
        ],
        "plugins": [
          "unassert",
          "lodash"
        ]
      }
    }
  },
  "dependencies": {
    "fallback-preload": "github:sasaplus1-prototype/fallback-preload.js"
  },
  "devDependencies": {
    "amp": "file:local_modules/amp",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-unassert": "^2.1.2",
    "babel-preset-env": "^1.4.0",
    "eslint": "^3.15.0",
    "fixpack": "^2.3.1",
    "html-loader": "^0.4.4",
    "husky": "^0.13.4",
    "json-loader": "^0.5.4",
    "new": "file:local_modules/new",
    "npm-run-all": "^4.1.1",
    "publish": "file:local_modules/publish",
    "stylelint": "^7.8.0",
    "stylelint-config-standard": "^16.0.0",
    "stylelint-processor-ignore-front-matter": "^0.1.0",
    "uglify-save-license": "^0.4.1",
    "webpack": "^2.2.1"
  },
  "engines": {
    "node": ">=8.4.0",
    "npm": ">=5.4.1"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "sourceType": "module"
    },
    "root": true
  },
  "private": true,
  "scripts": {
    "test": "make test"
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "processors": "stylelint-processor-ignore-front-matter"
  }
}
