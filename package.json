{
  "devDependencies": {
    "axios": "^0.18.0",
    "eslint": "^4.18.2",
    "eslint-plugin-vue": "^4.3.0",
    "fixpack": "^2.3.1",
    "highlight.js": "^9.12.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.0",
    "markdown-it": "^8.4.1",
    "nuxt": "^1.4.0",
    "nuxt-blog": "^1.0.0"
  },
  "engines": {
    "node": ">=8.10.0",
    "npm": ">=5.6.0"
  },
  "eslintConfig": {
    "env": {
      "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": "2018"
    },
    "root": true
  },
  "lint-staged": {
    "**/*.{js,vue}": "eslint",
    "package.json": [
      "fixpack",
      "git diff --exit-code --quiet"
    ]
  },
  "private": true,
  "scripts": {
    "precommit": "lint-staged"
  }
}
