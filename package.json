{
  "dependencies": {
    "domready": "^1.0.8",
    "fallback-preload": "github:sasaplus1-prototype/fallback-preload.js",
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-es3-member-expression-literals": "^6.22.0",
    "babel-plugin-transform-es3-property-literals": "^6.22.0",
    "babel-plugin-transform-strict-mode": "^6.22.0",
    "babel-plugin-transform-undefined-to-void": "^6.8.0",
    "babel-plugin-unassert": "^2.1.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-power-assert": "^1.0.0",
    "eslint": "^3.15.0",
    "fixpack": "^2.3.1",
    "html-loader": "^0.4.4",
    "json-loader": "^0.5.4",
    "npm-run-all": "^4.0.1",
    "power-assert": "^1.4.2",
    "stylelint": "^7.8.0",
    "stylelint-config-standard": "^16.0.0",
    "textlint": "^7.2.2",
    "textlint-filter-rule-comments": "^1.2.2",
    "textlint-rule-preset-ja-technical-writing": "^1.0.0",
    "uglify-save-license": "^0.4.1",
    "webpack": "^2.2.1"
  },
  "private": true,
  "scripts": {
    "compile": "npm run webpack:compile",
    "develop": "npm run webpack:develop",
    "eslint": "eslint ./javascript",
    "fixpack": "fixpack",
    "minify": "npm run webpack:minify",
    "stylelint": "find ./css ./_sass -type f -name '*.scss' -print0 | xargs -0 -n 1 -I % bash -c 'grep -v ^---$ % | stylelint --stdin-filename %'",
    "test": "run-s textlint eslint stylelint",
    "textlint": "textlint ./_drafts ./_posts",
    "webpack": "webpack --colors --display-error-details --progress --module-bind json",
    "webpack:compile": "npm run webpack",
    "webpack:develop": "npm run webpack -- --watch",
    "webpack:minify": "NODE_ENV=production npm run webpack:compile -- --optimize-minimize"
  }
}
